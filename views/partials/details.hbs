{{#if forecast}}
  <table class="table">
    <thead>
      <tr>
        <th>Date</th>
        <th>Low</th>
        <th>High</th>
        <th>Text</th>
      </tr>
    </thead>
    <tbody>
    {{#each forecast}}
      <tr>
        <td>{{date}}</td>
        <td>{{low}}</td>
        <td>{{high}}</td>
        <td>{{text}}</td>
      </tr>
    {{/each}}
    </tbody>
  </table>
{{else}}

{{/if}}
